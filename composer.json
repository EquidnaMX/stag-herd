{
    "name": "equidna/stag-herd",
    "description": "Multi-provider payment webhook verification and processing for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Equidna Development Team",
            "email": "dev@equidna.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "equidna/laravel-toolkit": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5",
        "infection/infection": "^0.31.9",
        "friendsofphp/php-cs-fixer": "^3.91",
        "rector/rector": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Equidna\\StagHerd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Equidna\\StagHerd\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Equidna\\StagHerd\\StagHerdServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan analyse"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
